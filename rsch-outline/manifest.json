{
    "name": "Research Assistant",
    "version": "1.0.0",
    "description": "Store your sources while researching",
    "manifest_version": 2,
    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_title": "Research Assistant",
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "content_security_policy": "script-src 'sha256-822b9066b37d0fbe1ecfd2cfbc671b4f'; object-src 'self'"
}